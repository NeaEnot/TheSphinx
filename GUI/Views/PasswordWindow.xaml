<Window x:Class="TheSphinx.GUI.Views.PasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TheSphinx.GUI.Views"
        mc:Ignorable="d"
        Title="Enter password" MinHeight="120" MinWidth="400" MaxHeight="120" MaxWidth="400">
    <Grid>
        <PasswordBox Name="passwordBox" Margin="10" VerticalAlignment="Top"/>
        <Button Content="Enter" HorizontalAlignment="Center" Margin="0,47,0,0" VerticalAlignment="Top" Height="27" Width="98" Click="Button_Click"/>

    </Grid>
</Window>
