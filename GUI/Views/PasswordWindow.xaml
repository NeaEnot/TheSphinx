<Window x:Class="GUI.Views.PasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI.Views"
        mc:Ignorable="d"
        Title="Введите пароль" MinHeight="100" MinWidth="400" MaxHeight="100" MaxWidth="400">
    <Grid>
        <PasswordBox Name="passwordBox" Margin="10" VerticalAlignment="Top"/>
        <Button Content="Enter" HorizontalAlignment="Center" Margin="10,10,10,23" VerticalAlignment="Bottom" Height="27" Width="98" Click="Button_Click"/>

    </Grid>
</Window>
